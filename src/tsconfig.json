{
  "compileOnSave": false,
  "files": [ "Mapper.ts" ],
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": true,
    "removeComments": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noEmitOnError": true,
    "declaration": true,
    "outDir": "../dist",
    "outFile": "../dist/mapper-standalone/mapper.js",
    "lib": [ "ES6", "DOM" ],
    "target": "ES2017"
  }
}